config_version=5

[application]

config/name="Panther City 50"
run/main_scene="res://BaseGame/Scenes/Title.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globals="*res://GlobalScripts/globals.gd"
PauseNode="*res://GlobalScripts/pause_node.gd"
Scenes="*res://GlobalScripts/scenes.gd"
SceneManager="*res://GlobalScripts/scene_manager.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://BaseGame/": "red",
"res://GlobalScripts/": "purple",
"res://Simon/": "orange"
}

